let ctrue a b = if true then a else b

let cfalse a b = if true then b else a

let cand (f:'a -> 'a -> 'a) (g:'a -> 'a -> 'a) = if (f = ctrue && g = ctrue) then (ctrue:'a -> 'a -> 'a) else cfalse

